<%- model_class = Areaimage -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:area_id) %>:</strong></dt>
  <dd><%= @areaimage.area_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:areaimage) %>:</strong></dt>
  <dd><%= @areaimage.areaimage %></dd>
  <dt><strong><%= model_class.human_attribute_name(:map_id) %>:</strong></dt>
  <dd><%= @areaimage.map_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              areaimages_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_areaimage_path(@areaimage), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              areaimage_path(@areaimage),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
